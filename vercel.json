{
    "version": 2,
    "builds": [
      { "src": "django_project/wsgi.py", "use": "@vercel/python" },
      { "src": "django_project/staticfiles/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/django_project/staticfiles/$1" },
      { "src": ".*", "dest": "django_project/wsgi.py" }
    ]
  }
  